                                                                                                                                                                          {
                                                                                                                                                                              if ((mitem.PerKey == "") || (PDALEngine.PDALSect.GetCheckPer(mitem.PerKey, null, null) == PDALEngine.PDALSect.AccessResult.Permitted))
